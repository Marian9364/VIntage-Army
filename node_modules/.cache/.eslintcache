/components/header/Header.js",["151","152"],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/main-page/MainPage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/register-page/RegisterPage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/footer/Footer.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/contact-page/ContactPage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/about-page/AboutPage.js",["153"],[],"import creator from \"../../img/creator.jpg\";\nimport styles from \"./AboutPage.module.css\";\n\nexport function AboutPage() {\n  return (\n    <div className={styles.mainWrapper}>\n        <img className={styles.img} src={creator} alt=\"Creator's Image\" width=\"300\" height=\"400\" />\n        <div className={styles.textWrapper}>\n        <h1 className={styles.heading}>About BG Baba</h1>\n        <p>Web application made by me as a SoftUni project.</p>\n        <p>The main pourpose of this web application is to connect hardworking grandmothers all over Bulgaria with their clients around the globe.</p>\n        <p>This is just the template and in near future, there are going to be added many more functionalities.</p>\n        </div>\n\n    </div>\n  );\n}","/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/login-page/LoginPage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/all-items-page/AllItemsPage.js",["154"],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/profile-page/ProfilePage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/edit-page/EditItemPage.js",["155"],[],"import styles from \"./EditItemPage.module.css\";\nimport { useState, useEffect } from \"react\";\nimport { useParams, useNavigate } from 'react-router-dom';\nimport * as itemsService from \"../../services/itemsService\";\nimport { useItemContext } from \"../../contexts/ItemContext\";\n\nexport const EditItemPage = () => {\n \n  const [currentItem, setCurrentItem] = useState({});\n  const { editItem } = useItemContext();\n  const { itemId } = useParams();\n  const navigate = useNavigate();\n\n  useEffect(() => {\n    itemsService.getOne(itemId)\n        .then(itemData => {\n            setCurrentItem(itemData);\n        })\n}, [])\n\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n\n    const itemData = Object.fromEntries(new FormData(e.target));\n\n    itemsService.edit(itemId, itemData)\n        .then(result => {\n            editItem(itemId, result);\n            navigate(\"/items\")\n        });\n  };\n\n  return (\n    <div className={styles.mainWrapper}>\n      <div className={styles.innerWrapper}>\n        <h1 className={styles.heading}>Edit Item</h1>\n        <form onSubmit={submitHandler}>\n          <div>\n            <label htmlFor=\"type\">Type:</label>\n            <input\n              className={styles.inputFields}\n              id=\"type\"\n              type=\"text\"\n              name=\"type\"\n              defaultValue={currentItem.type}\n             />\n          </div>\n          <div>\n            <label htmlFor=\"size\">Size:</label>\n            <select\n              className={styles.inputFields}\n              name=\"size\"\n              id=\"size\"\n              value={currentItem.size}\n              onChange={(e) => setCurrentItem({ ...currentItem, size: e.target.value })}\n            >\n              <option value=\"XS\">XS</option>\n              <option value=\"S\">S</option>\n              <option value=\"M\">M</option>\n              <option value=\"XS-M\">XS-M</option>\n              <option value=\"L\">L</option>\n              <option value=\"XL\">XL</option>\n              <option value=\"XXL\">XXL</option>\n              <option value=\"L-XXL\">L-XXL</option>\n              <option value=\"36-41\">36-41(for socks)</option>\n              <option value=\"42-46\">42-46(for socks)</option>\n            </select>\n          </div>\n          <div>\n            <label htmlFor=\"gender\">Gender:</label>\n            <select\n              className={styles.inputFields}\n              name=\"gender\"\n              id=\"gender\"\n              value={currentItem.gender}\n              onChange={(e) => setCurrentItem({ ...currentItem, gender: e.target.value })}\n\n            >\n              <option value=\"Male\">Male</option>\n              <option value=\"Female\">Female</option>\n              <option value=\"Unisex\">Unisex</option>\n            </select>\n          </div>\n          <div>\n          <label htmlFor=\"img\">Image:</label>\n          <input \n            className={styles.inputFields}\n            type=\"text\" \n            defaultValue={currentItem.img}\n            name=\"img\"\n            />\n          </div>\n          <div>\n            <label htmlFor=\"price\">Price in Leva:</label>\n            <input\n              className={styles.inputFields}\n              id=\"price\"\n              type=\"number\"\n              name=\"price\"\n              defaultValue={currentItem.price}\n              step=\"0.01\"\n            />\n          </div>\n          <div>\n            <label htmlFor=\"creator\">Creator:</label>\n            <input\n              className={styles.inputFields}\n              id=\"creator\"\n              type=\"text\"\n              name=\"creator\"\n              defaultValue={currentItem.creator}\n            />\n          </div>\n          <div>\n            <label htmlFor=\"phone\">Phone number:</label>\n            <input\n              className={styles.inputFields}\n              id=\"phone\"\n              type=\"number\"\n              name=\"phone\"\n              defaultValue={currentItem.phone}\n            />\n          </div>\n          <div>\n            <button className={styles.editItemBtn} type=\"submit\">Edit Item</button>\n          </div>\n        </form>\n      </div>\n    </div>\n  );\n};\n","/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/add-item-page/AddItemPage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/logout/Logout.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/item-details-page/ItemDetailsPage.js",["156","157","158","159"],[],"import styles from \"./ItemDetailsPage.module.css\";\nimport \"../../reset.css\";\nimport { useEffect } from \"react\";\nimport { useParams, Link, useNavigate } from \"react-router-dom\";\nimport * as itemsService from \"../../services/itemsService\";\nimport * as commentService from \"../../services/commentService\";\nimport { useItemContext } from \"../../contexts/ItemContext\";\nimport { useAuthContext } from \"../../contexts/AuthContext\";\n\nexport const ItemDetailsPage = () => {\n  const { itemId } = useParams();\n  const { user, isAuthenticated } = useAuthContext();\n  const navigate = useNavigate();\n  const { addComment, fetchItemDetails, selectItem, removeItem } =\n    useItemContext();\n  const currentItem = selectItem(itemId);\n\n  const isOwner = currentItem._ownerId === user._id\n\n  useEffect(() => {\n    (async () => {\n      const itemDetails = await itemsService.getOne(itemId);\n      const itemComments = await commentService.getByItemId(itemId);\n\n      console.log(user._id == itemDetails._ownerId);\n\n      fetchItemDetails(itemId, {\n        ...itemDetails,\n        comments: itemComments.map((x) => `${x.user.email}: ${x.text}`),\n      });\n    })();\n  }, []);\n\n  const addCommentHandler = (e) => {\n    e.preventDefault();\n    const formData = new FormData(e.target);\n\n    const comment = formData.get(\"comment\");\n    if (comment === \"\") {\n      alert(\"Comment can not be empty!\");\n      return;\n    }\n\n    commentService.create(itemId, comment).then((result) => {\n      addComment(itemId, comment);\n    });\n    e.target.reset();\n  };\n\n  const deleteItemHandler = () => {\n    const confirmation = window.confirm(\n      \"Are you sure you want to delete the item?\"\n    );\n\n    if (confirmation) {\n      itemsService.remove(itemId).then(() => {\n        removeItem(itemId);\n        navigate(\"/items\");\n      });\n    }\n  };\n\n  return (\n    <div className={styles.mainWrapper}>\n      <div className={styles.innerWrapper}>\n        <h1 className={styles.heading}>Item Details</h1>\n        <h1>Item Type: {currentItem.type}</h1>\n        <img className={styles.img} src={currentItem.img} />\n        <h4>Size: {currentItem.size}</h4>\n        <h4>Gender: {currentItem.gender}</h4>\n        <h4>Price: {currentItem.price}</h4>\n        <h2>Created By: {currentItem.creator}</h2>\n        <h2>Creator's Phone Number: {currentItem.phone}</h2>\n        {isOwner && \n          <div className={styles.btns}>\n            <Link to={`/items/${itemId}/edit`} className={styles.editBtn}>\n              Edit\n            </Link>\n            <button onClick={deleteItemHandler} className={styles.delBtn}>\n              Delete\n            </button>\n          </div>\n        }        \n        <div className={styles.commentsWrapper}>\n          <h2 className={styles.commentsText}>Comments:</h2>\n          <ul role=\"list\">\n            {currentItem.comments?.map((x) => (\n              <li key={x} className={styles.comment}>\n                <p>{x}</p>\n              </li>\n            ))}\n          </ul>\n          {!currentItem.comments && <p>No comments.</p>}\n        </div>\n        <div className={styles.createComment}>\n          <label>Comment On Other Sellers:</label>\n          <div className={styles.commentFieldAndBtn}>\n            <form className={styles.form} onSubmit={addCommentHandler}>\n              <textarea name=\"comment\" placeholder=\"Comment......\" />\n              {isOwner || !isAuthenticated || !user?\n              <button className={styles.btnSubmitDisabled} type=\"submit\" disabled> No Comment</button>\n              :\n              <button className={styles.btnSubmit} type=\"submit\"> Add Comment</button>\n              }\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n","/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/not-found-page/NotFoundPage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/route-guard/RouteGuard.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/hooks/useLocalStorage.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/services/commentService.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/services/authService.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/services/itemsService.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/main-page/main-page-components/Headings.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/main-page/main-page-components/MainContent.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/single-item/SingleItem.js",["160"],[],"import { Link } from \"react-router-dom\";\nimport styles from \"./SingleItem.module.css\";\n\nexport const SingleItem = ({ item }) => {\n  return (\n    <div className={styles.mainWrapper}>\n      <div className={styles.innerWrapper}>\n        <h1 className={styles.heading}>Item Type: {item.type}</h1>\n        <div className={styles.imgContainer}>\n        <img className={styles.img} src={item.img} />\n        </div>\n        <h4>Size: {item.size}</h4>\n        <h4>Gender: {item.gender}</h4>\n        <h4 className={styles.price}>Price: {item.price}</h4>\n        <Link to={`/items/${item._id}`} className={styles.detailsBtn}>Details and Contacts</Link>\n      </div>\n    </div>\n  );\n};\n","/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/services/requester.js",[],[],"/Users/mariyannikolov/Documents/GitHub/Vintage-Army/src/components/all-items-page/SearchComponent.js"